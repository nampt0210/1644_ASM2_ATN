<%- include('../template/header.ejs') %>
    <div id="layoutSidenav">
        <%- include('side-nav.ejs') %>
            <div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid">
                        <h1 class="mt-4">Dashboard</h1>
                        <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item active">
                                <%if(typeof employee !=='undefined' ){ %>
                                    Hello, <%= employee.name%>
                                        <% }%>
                            </li>
                        </ol>
                        <div class="row">
                            <div class="col-xl-3 col-md-6">
                                <div class="card bg-primary text-white mb-4">
                                    <div class="card-body">5 Offices</div>
                                    <div class="card-footer d-flex align-items-center justify-content-between">
                                        <a class="small text-white stretched-link" href="#">View Details</a>
                                        <div class="small text-white"><i class="fas fa-angle-right"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-3 col-md-6">
                                <div class="card bg-warning text-white mb-4">
                                    <div class="card-body">12 Orders</div>
                                    <div class="card-footer d-flex align-items-center justify-content-between">
                                        <a class="small text-white stretched-link" href="#">View Details</a>
                                        <div class="small text-white"><i class="fas fa-angle-right"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-3 col-md-6">
                                <div class="card bg-success text-white mb-4">
                                    <div class="card-body">20 Employee</div>
                                    <div class="card-footer d-flex align-items-center justify-content-between">
                                        <a class="small text-white stretched-link" href="#">View Details</a>
                                        <div class="small text-white"><i class="fas fa-angle-right"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-3 col-md-6">
                                <div class="card bg-danger text-white mb-4">
                                    <div class="card-body">40 Customers</div>
                                    <div class="card-footer d-flex align-items-center justify-content-between">
                                        <a class="small text-white stretched-link" href="#">View Details</a>
                                        <div class="small text-white"><i class="fas fa-angle-right"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-4">
                            


                            <div class="card-header">
                                <i class="fas fa-table mr-1"></i>
                                Orders
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                        <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Product</th>
                                                <th>Quantity</th>
                                                <th>Price</th>
                                                <th>Created At</th>
                                                <th>Employee</th>
                            
                                                <!-- <th>More</th> -->
                                            </tr>
                                        </thead>
                            
                                        <tbody>
                            
                                            <%if(typeof orders !=='undefined' ) { orders.forEach((order)=> { %>
                                                <tr>
                                                    <td>
                                                        <%= order.dataValues.id%>
                                                    </td>
                                                    <td>
                                                        <%= order.dataValues.Product.name %>
                                                    </td>
                                                    <td>
                                                        <%= order.dataValues.quantity %>
                                                    </td>
                                                    <td>
                                                        <%= order.dataValues.price %>
                                                    </td>
                                                    <td>
                                                        <%= order.dataValues.createdAt %>
                                                    </td>
                                                    <td>
                                                        <%= order.dataValues.Employee.name %>
                                                    </td>
                            
                                                </tr>
                                                <% })} %>
                            
                            
                                        </tbody>
                                    </table>
                                </div>                        
                                
                            </div>
                            
                        </div>
                    </div>
                </main>
            </div>
    </div>
    </body>
